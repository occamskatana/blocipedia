<div class="border-form-div-1" %>
	<%= simple_form_for @wiki do |w| %>

		
			
			<%= w.text_field :title, class: 'form-control', placeholder: "Enter Title Here" %> <br />

			
			<%= w.text_area :body, rows: 15, class: 'form-control' %><br />

			<%= w.input :public, as: :radio_buttons %>

			<%= w.submit "Submit", class: 'btn btn-success' %>
</div>
	<% end %>
