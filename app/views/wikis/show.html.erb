  

<h1 align="center"><%= @wiki.title %></h1><br />

<div class="container" align="center">
	<div class="row">
		<h3>Collaborators:</h3><h4><%@wiki.users.each do |x| %><strong><%= x.name %>
		&nbsp;&nbsp;</strong><% end %></h4>
		<div class="col-md-12 well">
			<%= markdown(@wiki.body) %>
		</div>

		<div class="pull-left">
				<%= link_to "Delete", @wiki, method: :delete, class: 'btn btn-danger', data: {confirm: 'Are you complete sure you want to delete this wiki?'} %>
		
				<%= link_to "Add New Collaborator", new_collaborator_path(wiki_id: @wiki.id), class: 'btn btn-info' %>
		
				<%= link_to "Edit", edit_wiki_path(@wiki), class: 'btn btn-warning' %>
				<%= link_to "To Wikis Index", wikis_path, class: 'btn btn-primary' %>
		</div>
	</div>
</div>