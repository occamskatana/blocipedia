
	<% @wikis.each do |w| %>
	<div class="well">
		<div class="pull-right"> 
				<%= link_to "More", wiki_path(w), class: 'btn btn-primary' %>
		</div>
		<div class="pull-left">
				<%= link_to "Delete", w, method: :delete, class: 'btn btn-danger', data: {confirm: 'Are you complete sure you want to delete this wiki?'} %>
		</div>

		<div class="center">
				<%= link_to "Edit", edit_wiki_path(w), class: 'btn btn-info' %>
		</div>
		<h3><%= w.title %></h3>
		<p><%=truncate(w.body, :length => 250)%>...</p>
		
		
	</div>
	<% end %>
